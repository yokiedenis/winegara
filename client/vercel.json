{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1",
      "headers": {
        "Access-Control-Allow-Origin": "https://winegarage1.vercel.app/",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/index.html"
    }
  ]
}
